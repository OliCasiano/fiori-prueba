<mvc:View controllerName="capgemini.invoice.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <Panel id="_IDGenPanel1" headerText="{i18n>selectionScreen}">
                <Label id="labelShipName" labelFor="shipName" text="{i18n>shipName}" class="sapUISmallMargin"/>
                <Input  id="shipName" width="200px" class="sapUISmallMarginEnd"/>
                <Label  id="labelCountry" text="{i18n>country}" class="sapUISmallMarginEnd"/>
            </Panel>
        </content>
    </Page>
</mvc:View>
